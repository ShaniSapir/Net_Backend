{
    "env": {
        "browser": false,
        "es2021": true,
        "jest": true 
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": ["error", 2],
        "eol-last":["off"],
        "linebreak-style": ["error", "windows"],
        "prefer-destructuring":["off"],
        "no-var" : ["off"],
        "no-console" : ["off"]
         

          
    }
}
